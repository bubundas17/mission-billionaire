<% include ../../inc/usercpHead %>
<% include ../../inc/usercpsider %>
<div class="list-group">
    <div class="list-group-item active">
        <h5>Withdrawal Requests</h5>
    </div>
    <div class="list-group-item">
        <table class="table table-hover">
            <thead>
            <tr>
                <th>Amount</th>
                <th>TKC Address</th>
                <th>Status</th>
            </tr>
            </thead>
            <% widwrawls.forEach( token => { %>
            <tbody>
            <tr>
                <td><strong><%= token.amount %> TKC</strong></td>
                <td><span><%= token.tkcaddress %></span></td>
                <td><span><%= token.status %></span></td>
            </tr>
            </tbody>
            <% }) %>
        </table>
    </div>
</div>


<div class="list-group">
    <div class="list-group-item active">
        <h5>Recharge Requests</h5>
    </div>
    <div class="list-group-item">
        <table class="table table-hover">
            <thead>
            <tr>
                <th>Mo. Number</th>
                <th>INR</th>
                <th>TKC</th>
                <th>Status</th>
            </tr>
            </thead>
            <% recharges.forEach( token => { %>
            <tbody>
            <tr>
                <td><strong><%= token.info.number %></strong></td>
                <td><span><%= token.info.amount %> INR</span></td>
                <td><span><%= token.amount %> TKC</span></td>
                <td><span><%= token.status %></span></td>
            </tr>
            </tbody>
            <% }) %>
        </table>
    </div>
</div>

<% include ../../inc/footer %>
